[server]
url = "url"
token = "token"
tick_rate_ms = 1000
auto_reconnect = true
timeout_seconds = 10

[renderer]
target_fps = 0
vsync = true
resolution = [1280, 720]
window_mode = "windowed"
anisotropic_filtering = 16
anti_aliasing = "msaa4"
ssao_enabled = true
clear_color = [0.0, 0.0, 0.0]
wireframe_enabled = false
wireframe_color = [1.0, 1.0, 1.0]

[camera]
movement_speed = 15.0
sprint_multiplier = 2.0
mouse_sensitivity = 0.5
zoom_speed = 50.0
min_zoom = 5.0
max_zoom = 50.0
current_zoom = 20.0
drag_sensitivity = 0.01

[ui]
show_fps = true
show_connection = true
show_debug_text = true
show_game_state = true
enable_docking = true
menu_font_size = 16.0
ui_font_size = 20.0
menu_title = "Debug Menu"

[debug]
debug_mode = false
log_level = "info"
